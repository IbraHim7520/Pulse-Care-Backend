model MedicalReport {
    id String @id @default(uuid())
    reportName String
    reportLink String

    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt

    paitentId String
    paitent Paitent @relation(fields: [paitentId], references: [id], onDelete: Cascade)

    @@index([paitentId])
    @@map("medical_reports")
}